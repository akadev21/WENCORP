<odoo>
    <data noupdate="1">
        <record id="seq_usine_project" model="ir.sequence">
            <field name="name">Usine Project Sequence</field>
            <field name="code">usine.project</field>
            <field name="prefix">PROD-</field>
            <field name="padding">5</field>
        </record>
        <record id="commercial_project_sequences_generating" model="ir.sequence">
        <field name="name">project sequence</field>
        <field name="code">commercial.project</field>
        <field name="prefix">PRO-%(year)s%(month)s%(day)s-</field>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
        <field name="padding">3</field>
    </record>
    </data>

</odoo>
